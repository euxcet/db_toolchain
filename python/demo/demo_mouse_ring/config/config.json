{
  "gesture_detector": {
    "name": "GestureDetector",
    "num_classes": 5,
    "checkpoint_file": "checkpoint/touch_all.pth",
    "execute_interval": 10,
    "label": ["touch_down", "touch_up", "move", "click", "double_click"],
    "confidence_threshold": [0.9, 0.9, 0.9, 0.9, 0.9],
    "trigger_wait_time": [0, 0, 0, 0.3, 0],
    "block": {
      "click": {
        "gesture": ["touch_down"],
        "time": [1]
      },
      "double_click": {
        "gesture": ["touch_down", "click"],
        "time": [1, 1]
      }
    }
  },
  "trajectory_detector": {
    "name": "TrajectoryDetector",
    "gesture_detector_name": "GestureDetector",
    "checkpoint_file": "checkpoint/trajectory_all.pth",
    "execute_interval": 10
  },
  "mouse_ring": {
    "name": "MouseRing",
    "control_cursor": true,
    "cursor_initial_point_x": 330,
    "cursor_initial_point_y": 396,
    "trajectory_detector_name": "TrajectoryDetector",
    "gesture_detector_name": "GestureDetector"
  }
}